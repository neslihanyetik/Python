   ],
   "source": [
    "def my_min(*nums):\n",
    "    listt = list(nums)\n",
    "            \n",
    "    return min(listt)\n",
    "\n",
    "print(my_min(5,6,7))\n",
    "\n",
    "print(my_min(3,8,-9,0,12,1.2))\n",
    "\n",
    "print(my_min(-100))"
   ]
